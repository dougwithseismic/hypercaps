{
  "author": {
    "email": "support@withseismic.com",
    "name": "Seismic"
  },
  "build": {
    "appId": "com.hypercaps.app",
    "asar": true,
    "buildDependenciesFromSource": false,
    "directories": {
      "buildResources": "build"
    },
    "extraResources": [
      {
        "filter": [
          "*.ps1"
        ],
        "from": "electron/features/hyperkeys/scripts",
        "to": "electron/features/hyperkeys/scripts"
      }
    ],
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "linux": {
      "target": null
    },
    "npmRebuild": false,
    "win": {
      "forceCodeSigning": false,
      "sign": null,
      "target": "dir"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@hypercaps/ipc": "workspace:*",
    "@hypercaps/message-queue": "workspace:*",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "^1.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-menubar": "^1.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-router": "^1.95.3",
    "@tanstack/router-vite-plugin": "^1.95.3",
    "@types/uuid": "^10.0.0",
    "cmdk": "1.0.0",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0",
    "embla-carousel-react": "^8.5.2",
    "immer": "^10.1.1",
    "input-otp": "^1.4.2",
    "next-themes": "^0.4.4",
    "node-global-key-listener": "^0.3.0",
    "react": "^18.2.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.0",
    "sonner": "^1.7.1",
    "uuid": "^11.0.5",
    "vaul": "^1.1.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "description": "A powerful keyboard remapping tool for Windows",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@shadcn/ui": "^0.0.4",
    "@tanstack/router-devtools": "^1.95.3",
    "@tanstack/router-plugin": "^1.95.3",
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^4.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.12.0",
    "lucide-react": "^0.471.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.7",
    "vite-plugin-electron": "^0.15.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=18",
    "os": "win32"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "main": "dist-electron/main/main.js",
  "name": "hypercaps",
  "os": [
    "win32"
  ],
  "private": true,
  "scripts": {
    "build": "vite build && electron-builder",
    "dev": "cross-env NODE_ENV=development vite dev",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=development electron ."
  },
  "version": "0.2.0"
}
